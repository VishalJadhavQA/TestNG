package tutorials.demo;

import javax.print.DocFlavor.CHAR_ARRAY;

public class test {

	public static void main(String[] args) {

		
		
		String str= "Geeks";
		
		String nstr = "";
        
        for (int i = 0; i < str.length(); i++) {
			char ch = str.charAt(i);
			
			nstr = ch+nstr;
						
		}
        	
        System.out.println(nstr);
        
        
			
		}

	}


